{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CurrencyExchangeSettings": {
    "Frankfurter": {
      "BaseUrl": "https://api.frankfurter.dev/v1/latest"
    },
    "CurrenciesToPull": [ "EUR" ],
    "CurrentCurrency": "HUF",
    "CurrencyCacheKey": "CurrencyCacheKey"
  },
  "Serilog": {
    "MinimumLevel": "Error",
    "Enrich": [ "FromLogContext"],
    "Properties": {
      "ApplicationName": "Warehouse"
    }
  }

}
